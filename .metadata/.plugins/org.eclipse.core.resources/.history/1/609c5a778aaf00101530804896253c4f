package com.medica.testServiceImpl;

import com.medica.model.Tbl_Especialidades;
import com.medica.service.impl.ClassEspecialidadimp;

public class TestEspecialidad {
	public static void main(String[] args) {
		Tbl_Especialidades espe = new Tbl_Especialidades();
		ClassEspecialidadimp crud = new ClassEspecialidadimp();
		
		//REGISTRAR
		espe.setEspecialidad("Cardiologia");
		espe.setDescEspecialidad("Rama de la medicina que se encarga del estudio, prevencion, diagnostico y tratamiento de las enfermedades del corazon");
		crud.RegistrarEspecialidad(espe);
	}
}
